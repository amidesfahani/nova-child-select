{
    "name": "amidesfahani/nova-child-select",
    "description": "Laravel nova child field.",
    "keywords": [
        "laravel",
        "nova",
        "select"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Amidesfahani\\ChildSelect\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amidesfahani\\ChildSelect\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
